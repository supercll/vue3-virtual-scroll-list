{
  "name": "vue3-virtual-scroll-list-component",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build --mode lib && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "nanoid": "^4.0.2",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.8",
    "vue3-virtual-scroll-list-component": "^0.0.3"
  },
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.umd.cjs",
  "module": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "keywords": [
    "Components",
    "vue3",
    "virtual-scroll-list",
    "TS"
  ],
  "exports": {
    "./dist/lib/style.css": "./dist/lib/style.css",
    "./css": "./dist/lib/style.css",
    ".": {
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.umd.cjs"
    }
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  }
}
